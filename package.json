{
  "name": "xc_icons",
  "version": "1.0.0",
  "description": "Base build tool for HTML/CSS",
  "keywords": [
    "xcodi",
    "admin"
  ],
  "author": {
    "name": "Tai Nguyen",
    "github": "https://taint1811.github.io"
  },
  "license": "ISC",
  "main": "js/index.js",
  "style": "css/all.css",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf js/{brand,flag,free,common} png sprites",
    "build": "npm-run-all --sequential build-*",
    "build-js-files": "node build/build-js-files.js",
    "build-svg-sprites": "node build/build-svg-sprites.js",
    "build-png": "node build/generate-png.js",
    "dist": "npm-run-all --sequential clean build"
  },
  "devDependencies": {
    "clean-css-cli": "^4.3.0",
    "concat": "^1.0.3",
    "mkdirp": "^0.5.1",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.3",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.12.1"
  },
  "files": [
    "css/",
    "fonts/",
    "js/",
    "png/",
    "scss/",
    "sprites/",
    "svg/",
    "index.d.ts"
  ],
  "dependencies": {
    "minipass": "^2.7.0",
    "sharp": "^0.23.0"
  }
}
